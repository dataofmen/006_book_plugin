  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "@xmldom/xmldom": "^0.8.11",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "node-fetch": "^2.7.0",
    "obsidian": "latest",
    "ts-node": "^10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  }
