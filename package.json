{
  "name": "obsidian-kr-book-plugin",
  "version": "1.0.0",
  "description": "Korean National Library book search plugin for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
